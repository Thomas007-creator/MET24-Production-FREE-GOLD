# =====================================================
# MET2.4 VPN Pipeline - Environment Configuration
# =====================================================

# VPN Configuration
VPN_ENABLED=true
VPN_REQUIRED=true
VPN_CHECK_INTERVAL=30000
VPN_TIMEOUT=10000

# Tailscale Configuration
TAILSCALE_ENABLED=true
TAILSCALE_AUTHKEY=tskey-auth-your-tailscale-key-here

# WireGuard Configuration
WIREGUARD_ENABLED=true
WIREGUARD_SERVER_URL=your-wireguard-server.example.com
WIREGUARD_INTERNAL_SUBNET=10.0.0.0/24

# VPN Protected Sync Configuration
VPN_PROTECTED_SYNC_ENABLED=true
VPN_PROTECTED_SYNC_REQUIRED=true
VPN_PROTECTED_SYNC_INTERVAL=60000
VPN_PROTECTED_SYNC_BATCH_SIZE=10
VPN_PROTECTED_SYNC_RETRY_ATTEMPTS=3
VPN_PROTECTED_SYNC_RETRY_DELAY=1000
VPN_PROTECTED_SYNC_ENCRYPTION_ENABLED=true

# Testing Configuration
TESTING_ENABLED=true
TESTING_INTERVAL=60000
VALIDATION_LEVEL=comprehensive
INCLUDE_VPN_TESTS=true
INCLUDE_SYNC_TESTS=true
INCLUDE_DATABASE_TESTS=true
INCLUDE_VECTOR_TESTS=true

# Supabase Configuration
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key
REACT_APP_SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# AI Configuration
REACT_APP_OPENAI_API_KEY=sk-your-openai-api-key
REACT_APP_ANTHROPIC_API_KEY=your-anthropic-api-key

# Database Configuration
POSTGRES_DB=met24_mbti_coach
POSTGRES_USER=met24user
POSTGRES_PASSWORD=met24password

# Development Configuration
NODE_ENV=development
CHOKIDAR_USEPOLLING=true

